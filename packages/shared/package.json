{
  "name": "@biblio-num/shared",
  "version": "1.0.0",
  "description": "Types partagés entre le back et le front",
  "author": "Ministère de l’intérieur",
  "exports": {
    ".": {
      "import": {
        "types": "./types",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./types",
        "default": "./dist/index.js"
      }
    }
  },
  "keywords": [
    "types",
    "dto"
  ],
  "license": "ISC",
  "main": "dist/index.js",
  "scripts": {
    "build": "run-s clean:dist build:types",
    "build:types": "tsc",
    "clean:dist": "rimraf dist types",
    "dev": "nodemon --watch src --exec 'pnpm run build:types'",
    "format": "eslint src/ --ext .ts --ignore-path ../../.gitignore --fix",
    "lint": "eslint src/ --ext .ts --ignore-path ../../.gitignore",
    "lint:staged": "eslint --ext .ts --ignore-path ../../.gitignore",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "types": "types/",
  "dependencies": {
    "@nestjs/mapped-types": "^2.0.2",
    "@nestjs/swagger": "^7.1.13",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0"
  },
  "devDependencies": {
    "@dnum-mi/ds-api-client": "^1.6.2",
    "@nestjs/common": "^10.2.7",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  },
  "volta": {
    "node": "18.16.0"
  }
}
