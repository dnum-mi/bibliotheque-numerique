{
  "name": "biblio-num-client",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "test": "run-s test:unit test:ct:ci",
    "test:unit": "vitest --environment jsdom",
    "test:ct": "cypress open --component",
    "test:ct:ci": "cypress run --component",
    "pretest:e2e": "npm run build",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "pretest:e2e:ci": "npm run build",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "format": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "docker:dev:build": "docker compose -f docker-compose.dev.yml build",
    "docker:dev": "docker compose -f docker-compose.dev.yml up",
    "docker:dev:stop": "docker compose -f docker-compose.dev.yml down",
    "docker:prod:build": "docker compose -f docker-compose.prod.yml build",
    "docker:prod": "docker compose -f docker-compose.prod.yml up -d",
    "docker:prod:stop": "docker compose -f docker-compose.prod.yml down"
  },
  "dependencies": {
    "@gouvminint/vue-dsfr": "^2.1.1",
    "@vee-validate/zod": "^4.7.3",
    "ag-grid-community": "^28.2.1",
    "ag-grid-enterprise": "^28.2.1",
    "ag-grid-vue3": "^28.2.1",
    "axios": "^1.3.1",
    "oh-vue-icons": "1.0.0-rc3",
    "pinia": "^2.0.30",
    "pretty-bytes": "^6.1.0",
    "vee-validate": "^4.7.3",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@cypress/vue": "^5.0.3",
    "@faker-js/faker": "^7.6.0",
    "@rushstack/eslint-patch": "^1.1.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "@testing-library/vue": "^6.6.1",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^17.0.31",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.3.0",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^12.5.0",
    "eslint": "^8.25.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.6.0",
    "jsdom": "^20.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "sass": "^1.56.1",
    "start-server-and-test": "^1.14.0",
    "typescript": "~4.9.5",
    "vite": "^4.1.1",
    "vite-plugin-pwa": "^0.14.1",
    "vitest": "^0.28.3",
    "vue-tsc": "^1.0.24"
  }
}
