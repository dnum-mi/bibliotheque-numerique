{
  "name": "biblio-num-client",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "test": "run-s test:unit test:ct-ci test:e2e-ci",
    "test:unit": "vitest --environment jsdom",
    "test:ct": "cypress open-ct",
    "test:ct:ci": "cypress run-ct",
    "pretest:e2e": "npm run build",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "pretest:e2e:ci": "npm run build",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "format": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@gouvminint/vue-dsfr": "1.4.0",
    "axios": "^1.0.0",
    "focus-trap": "^6.8.1",
    "focus-trap-vue": "^3.2.1",
    "oh-vue-icons": "1.0.0-rc3",
    "pinia": "^2.0.14",
    "vue": "^3.2.33",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^2.2.2",
    "@cypress/vue": "^3.1.2",
    "@rushstack/eslint-patch": "^1.1.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/user-event": "^14.1.1",
    "@testing-library/vue": "^6.5.1",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^17.0.31",
    "@vitejs/plugin-vue": "^2.3.2",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "2.0.0-rc.21",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^10.10.0",
    "eslint": "^8.15.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "jsdom": "^19.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "start-server-and-test": "^1.14.0",
    "typescript": "~4.6.4",
    "vite": "^2.9.8",
    "vite-plugin-pwa": "^0.12.0",
    "vitest": "^0.12.0",
    "vue-tsc": "^0.34.11"
  }
}
