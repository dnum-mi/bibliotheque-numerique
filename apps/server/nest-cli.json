{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "assets": ["**/*.ejs"],
    "deleteOutDir": true,
    "builder": "swc",
    "watchAssets": true,
    "typeCheck": true,
    "plugins": ["@nestjs/swagger"]
  }
}
