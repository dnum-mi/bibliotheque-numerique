{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "compilerOptions": {
    "assets": [
      "**/*.ejs"
    ],
    "builder": "swc",
    "plugins": [
      "@nestjs/swagger"
    ],
    "typeCheck": true,
    "watchAssets": true
  },
  "sourceRoot": "src"
}