{
  "name": "@biblio-num/server",
  "version": "1.0.0",
  "description": "",
  "author": "minint",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build": "nest build",
    "db:create-default-admin": "ts-node src/scripts/createDefaultAdmin.ts",
    "db:create-default-admin:prod": "node dist/scripts/createDefaultAdmin.js",
    "db:loadOrganismes": "node dist/scripts/loadOrganismes.js",
    "dev": "run-s dev:docker dev:server",
    "dev:docker": "docker compose --profile simple-dev -f docker-compose.dev.yml up -d",
    "dev:docker:stop": "docker compose --profile simple-dev -f docker-compose.dev.yml stop",
    "dev:in-docker": "run-s dev:server",
    "dev:server": "nest start --debug 0.0.0.0:9229 --watch",
    "dev:worker": "nest start --entryFile main-worker.js --debug 0.0.0.0:9230 --watch",
    "docker:build": "docker compose -f docker-compose.yml -f docker-compose.dev.yml build server",
    "docker:db:down": "docker compose -f docker-compose.yml down",
    "docker:db:up": "docker compose -f docker-compose.yml up -d",
    "docker:dev": "docker compose --profile dev -f docker-compose.dev.yml up -d",
    "docker:logs": "docker logs -f biblio-num_server",
    "docker:logs:db": "docker logs -f biblio-num_db",
    "docker:prod": "docker compose -f docker-compose.yml -f docker-compose.prod.yml up -d",
    "docker:prod:build": "docker compose -f docker-compose.yml -f docker-compose.prod.yml build",
    "docker:prod:stop": "docker compose -f docker-compose.yml -f docker-compose.prod.yml down",
    "docker:smtp:run": "docker compose -f docker-compose.yml -f docker-compose.dev.yml up mailhog -d",
    "docker:start": "docker compose -f docker-compose.yml up",
    "docker:stop": "docker compose -f docker-compose.yml -f docker-compose.dev.yml down",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path ../../.gitignore",
    "lint:fix": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path ../../.gitignore",
    "plugin:init": "cd src/plugins/ && sh bin/init",
    "prebuild": "rimraf dist",
    "script:loadOrganismes": "node dist/scripts/loadOrganismes.js",
    "sdev": "run-s dev:*",
    "sdev-stop": "run-s dev:docker:stop",
    "start": "nest start",
    "start:prod": "typeorm-ts-node-commonjs migration:run -d dist/db/app-data-source.js; node dist/main",
    "stop": "docker compose stop -d",
    "sync:demarches": "ts-node src/scripts/loadDemarcheDs.ts",
    "sync:demarches:prod": "node dist/scripts/loadDemarcheDs.js",
    "sync:dossiers": "ts-node src/scripts/loadDossierDs.ts",
    "sync:dossiers:prod": "node dist/scripts/loadDossierDs.js",
    "test": "NODE_ENV='test' jest --runInBand",
    "test:cov": "NODE_ENV='test' jest --coverage --runInBand",
    "test:debug": "NODE_ENV='test' node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV='test' jest --config ./test/jest-e2e.json",
    "test:watch": "NODE_ENV='test' jest --watch --runInBand",
    "typeorm": "NODE_ENV='test' typeorm-ts-node-commonjs",
    "typeorm:migration:create": "typeorm-ts-node-commonjs migration:create",
    "typeorm:migration:generate": "typeorm-ts-node-commonjs migration:generate src/db/migrations -d src/db/app-data-source.ts",
    "typeorm:migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/db/app-data-source.ts",
    "typeorm:migration:run": "typeorm-ts-node-commonjs migration:run -d src/db/app-data-source.ts",
    "typeorm:migration:show": "typeorm-ts-node-commonjs migration:show -d src/db/app-data-source.ts",
    "vite": "vite"
  },
  "dependencies": {
    "@biblio-num/utils": "1.0.0",
    "@dnum-mi/ds-api-client": "^1.3.0",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/axios": "2.0.0",
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/schedule": "^2.2.1",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/typeorm": "^9.0.1",
    "aws-sdk": "^2.1377.0",
    "axios": "1.4.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "0.14.0",
    "colors": "^1.4.0",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "express-session": "^1.17.3",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^2.10.0",
    "nodemailer": "^6.9.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.16",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@nestjs/cli": "^9.4.2",
    "@nestjs/schematics": "^9.1.0",
    "@nestjs/testing": "^9.4.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/jest": "28.1.8",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.16.9",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "mock-aws-s3": "^4.0.2",
    "nock": "^13.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.9.5",
    "vite-plugin-node": "^3.0.2",
    "vitest": "^0.30.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "coveragePathIgnorePatterns": [
      "db",
      "config"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "volta": {
    "node": "18.16.0"
  }
}