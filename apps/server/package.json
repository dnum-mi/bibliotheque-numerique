{
  "name": "@biblio-num/server",
  "version": "1.0.0",
  "description": "",
  "author": "minint",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build": "nest build",
    "clean:dist": "rimraf dist",
    "db:create-default-admin": "ts-node --require=tsconfig-paths/register src/shared/scripts/createDefaultAdmin.ts",
    "db:create-default-admin:prod": "node dist/src/shared/scripts/createDefaultAdmin.js",
    "db:loadOrganismes": "node dist/src/shared/scripts/loadOrganismes.js",
    "dev": "run-s dev:docker dev:server",
    "dev:docker": "docker compose --profile simple-dev -f docker-compose.yml up -d",
    "dev:docker:stop": "docker compose --profile simple-dev -f docker-compose.yml stop",
    "dev:in-docker": "run-s dev:server",
    "dev:server": "nest start --debug 0.0.0.0:9229 --watch",
    "dev:worker": "nest start --entryFile src/main-worker.js --debug 0.0.0.0:9230 --watch",
    "dev:worker-in-docker": "run-s dev:worker",
    "docker:build": "docker compose -f docker-compose.yml -f docker-compose.yml build server",
    "docker:db:down": "docker compose -f docker-compose.yml down",
    "docker:db:up": "docker compose -f docker-compose.yml up -d",
    "docker:dev": "docker compose --profile dev -f docker-compose.yml up -d",
    "docker:logs": "docker logs -f biblio-num_server",
    "docker:logs:db": "docker logs -f biblio-num_db",
    "docker:smtp:run": "docker compose -f docker-compose.yml -f docker-compose.yml up mailhog -d",
    "docker:start": "docker compose -f docker-compose.yml up",
    "docker:stop": "docker compose -f docker-compose.yml -f docker-compose.yml down",
    "fixtures": "fixtures-ts-node-esm load test/e2e/fixtures --require=tsconfig-paths/register --dataSource=./test/e2e/data-source-e2e.typeorm.ts --sync",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path ../../.gitignore",
    "lint:fix": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path ../../.gitignore",
    "lint:staged": "eslint --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path ../../.gitignore",
    "migration:generate": "npm run typeorm migration:generate",
    "migration:revert": "npm run typeorm migration:revert",
    "migration:run": "NODE_OPTIONS=\"-r tsconfig-paths/register\" npm run typeorm migration:run",
    "migration:show": "npm run typeorm migration:show",
    "plugin:init": "cd src/plugins/ && sh bin/init",
    "prebuild": "rimraf dist",
    "script:loadOrganismes": "node dist/src/shared/scripts/loadOrganismes.js",
    "sdev": "run-s dev:*",
    "sdev-stop": "run-s dev:docker:stop",
    "start": "nest start",
    "start:prod": "typeorm-ts-node-commonjs migration:run -d dist/database/data-source.typeorm.js; node dist/src/main",
    "start:worker:prod": "node dist/src/main-worker",
    "stop": "docker compose stop -d",
    "test": "run-s test:unit test:e2e",
    "test:e2e": "test/e2e/launch-e2e-test.sh",
    "test:e2e:dev": "test/e2e/launch-e2e-test.sh dev",
    "test:e2e:one": "NODE_ENV='test-e2e' dotenv -e ./test/.env.test jest -- --config ./test/e2e/jest-e2e.json",
    "test:unit": "test/unit/launch-unit-test.sh",
    "test:unit:dev": "test/unit/launch-unit-test.sh dev",
    "typeorm": "typeorm-ts-node-commonjs -d ./database/data-source.typeorm.ts",
    "vite": "vite"
  },
  "dependencies": {
    "@biblio-num/shared": "workspace:^",
    "@dnum-mi/ds-api-client": "^1.6.2",
    "@nest-middlewares/morgan": "7.0.1",
    "@nestjs-modules/mailer": "^1.9.1",
    "@nestjs/axios": "3.0.0",
    "@nestjs/common": "^10.2.1",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.2.1",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.1",
    "@nestjs/platform-express": "^10.2.1",
    "@nestjs/schedule": "^3.0.3",
    "@nestjs/terminus": "10.0.1",
    "@nestjs/typeorm": "^10.0.0",
    "aws-sdk": "^2.1443.0",
    "axios": "1.4.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "0.14.0",
    "colors": "^1.4.0",
    "cron": "2.4.1",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "dotenv-cli": "7.3.0",
    "ejs": "^3.1.9",
    "express-session": "^1.17.3",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "nodemailer": "^6.9.4",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@nestjs/cli": "^10.1.14",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/swagger": "^7.1.8",
    "@nestjs/testing": "^10.2.1",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.78",
    "@types/bcrypt": "^5.0.0",
    "@types/cron": "^2.4.0",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/jest": "29.5.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.17.0",
    "@types/nodemailer": "6.4.9",
    "@types/passport-local": "^1.0.35",
    "@types/superagent": "^4.1.18",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.3",
    "mock-aws-s3": "^4.0.2",
    "mockdate": "3.0.5",
    "nock": "^13.3.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typeorm-fixtures-cli": "3.1.0",
    "typescript": "^5.1.6",
    "vite-plugin-node": "^3.0.2",
    "vitest": "^0.34.2"
  },
  "volta": {
    "node": "18.16.0"
  }
}
