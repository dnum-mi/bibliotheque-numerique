{
  "name": "@biblio-num/client",
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "dev": "vite",
    "dev:e2e": "start-server-and-test 'vite dev --port 5050' http://127.0.0.1:5050/ 'cypress open --e2e'",
    "dev:e2e:ci": "start-server-and-test 'vite dev --port 5050' http://127.0.0.1:5050/ 'cypress run --e2e'",
    "docker:dev": "docker compose -f docker-compose.dev.yml up",
    "docker:dev:build": "docker compose -f docker-compose.dev.yml build",
    "docker:dev:stop": "docker compose -f docker-compose.dev.yml down",
    "docker:prod": "docker compose -f docker-compose.prod.yml up -d",
    "docker:prod:build": "docker compose -f docker-compose.prod.yml build",
    "docker:prod:stop": "docker compose -f docker-compose.prod.yml down",
    "format": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:staged": "eslint --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "pretest:e2e": "npm run build",
    "pretest:e2e:ci": "npm run build",
    "preview": "CYPRESS=true vite preview --port 5050",
    "test": "run-s test:unit test:ct:ci dev:e2e:ci",
    "test:ct": "CYPRESS=true cypress open --component",
    "test:ct:ci": "NODE_ENV=production CYPRESS=true cypress run --component",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run --e2e'",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "dependencies": {
    "@biblio-num/shared": "workspace:^",
    "@gouvfr/dsfr": "^1.11.0",
    "@gouvminint/vue-dsfr": "^5.5.0",
    "@vee-validate/zod": "^4.12.2",
    "@vueuse/core": "^10.7.0",
    "ag-grid-community": "^30.2.1",
    "ag-grid-enterprise": "^30.2.1",
    "ag-grid-vue3": "^30.2.1",
    "axios": "^1.6.2",
    "oh-vue-icons": "1.0.0-rc3",
    "pinia": "^2.1.7",
    "pretty-bytes": "^6.1.1",
    "sanitize-html": "2.11.0",
    "slugify": "^1.6.6",
    "vee-validate": "^4.12.2",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cypress/vue": "^6.0.0",
    "@dnum-mi/ds-api-client": "^1.6.2",
    "@faker-js/faker": "^8.3.1",
    "@rushstack/eslint-patch": "^1.6.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/user-event": "^14.5.1",
    "@types/sanitize-html": "2.9.5",
    "@testing-library/vue": "^8.0.1",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.10.4",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.4.0",
    "cypress": "^13.6.1",
    "eslint": "^8.55.0",
    "eslint-config-standard-with-typescript": "^42.0.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.19.2",
    "jsdom": "^23.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "sass": "^1.69.5",
    "start-server-and-test": "^2.0.3",
    "typescript": "~5.3.3",
    "unocss": "^0.58.0",
    "unplugin-auto-import": "^0.17.2",
    "unplugin-vue-components": "0.26.0",
    "vite": "^5.0.8",
    "vite-plugin-pwa": "^0.17.4",
    "vite-svg-loader": "^5.1.0",
    "vitest": "^1.0.4",
    "vue-tsc": "^1.8.25",
    "workbox-window": "^7.0.0"
  },
  "volta": {
    "node": "18.16.0",
    "npm": "8.19.4"
  }
}
