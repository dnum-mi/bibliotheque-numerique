{
  "name": "@biblio-num/client",
  "type": "module",
  "version": "2.40.0",
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "dev": "vite",
    "dev:e2e": "VITE_CYPRESS=true start-server-and-test 'vite dev --port 5050' http://127.0.0.1:5050/ 'cypress open --e2e'",
    "dev:e2e:ci": "VITE_CYPRESS=true start-server-and-test 'vite dev --port 5050' http://127.0.0.1:5050/ 'cypress run --e2e'",
    "docker:dev": "docker compose -f docker-compose.dev.yml up",
    "docker:dev:build": "docker compose -f docker-compose.dev.yml build",
    "docker:dev:stop": "docker compose -f docker-compose.dev.yml down",
    "docker:prod": "docker compose -f docker-compose.prod.yml up -d",
    "docker:prod:build": "docker compose -f docker-compose.prod.yml build",
    "docker:prod:stop": "docker compose -f docker-compose.prod.yml down",
    "format": "eslint . --fix",
    "generate-icons": "vue-dsfr-icons -s scripts/icons.js -t src/icon-collections.ts",
    "lint": "eslint .",
    "lint:staged": "eslint ",
    "pretest:e2e": "npm run build",
    "pretest:e2e:ci": "npm run build",
    "preview": "CYPRESS=true vite preview --port 5050",
    "test": "run-s test:unit test:ct:ci dev:e2e:ci",
    "test:ct": "CYPRESS=true cypress open --component",
    "test:ct:ci": "NODE_ENV=production CYPRESS=true cypress run --component",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run --e2e'",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "dependencies": {
    "@ag-grid-community/locale": "33.2.4",
    "@biblio-num/shared": "workspace:^",
    "@gouvfr/dsfr": "^1.14.1",
    "@gouvminint/vue-dsfr": "^8.8.0",
    "@vee-validate/zod": "^4.12.6",
    "@vue-leaflet/vue-leaflet": "0.10.1",
    "@vueuse/core": "^12.0.0",
    "ag-grid-community": "^31.3.4",
    "ag-grid-enterprise": "^31.3.4",
    "ag-grid-vue3": "^31.3.4",
    "axios": "^1.11.0",
    "leaflet": "1.9.4",
    "oh-vue-icons": "1.0.0-rc3",
    "ol": "10.6.1",
    "ol-contextmenu": "5.5.0",
    "ol-ext": "4.0.34",
    "pinia": "^3.0.1",
    "pretty-bytes": "^6.1.1",
    "sanitize-html": "^2.14.0",
    "shepherd.js": "14.5.1",
    "slugify": "^1.6.6",
    "vee-validate": "^4.14.7",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue3-openlayers": "^12.1.1",
    "zod": "^4.1.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@cypress/vue": "^6.0.2",
    "@dnum-mi/ds-api-client": "^1.8.0",
    "@faker-js/faker": "^10.0.0",
    "@iconify-json/fa": "1.2.2",
    "@iconify-json/ri": "1.2.5",
    "@iconify-json/vscode-icons": "1.2.30",
    "@iconify/vue": "4.3.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.5.2",
    "@testing-library/vue": "^8.1.0",
    "@tsconfig/node22": "22.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.18.0",
    "@types/sanitize-html": "2.16.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "cypress": "15.2.0",
    "eslint": "^9.22.0",
    "eslint-plugin-cypress": "^5.1.1",
    "jsdom": "^26.1.0",
    "msw": "2.10.5",
    "npm-run-all": "^4.1.5",
    "sass": "^1.81.0",
    "start-server-and-test": "^2.0.12",
    "typescript": "~5.8.3",
    "unocss": "^66.4.2",
    "unplugin-auto-import": "^20.1.0",
    "unplugin-vue-components": "^29.0.0",
    "vite": "^7.1.6",
    "vite-plugin-commonjs": "0.10.4",
    "vite-plugin-pwa": "^1.0.3",
    "vite-svg-loader": "^5.1.0",
    "vitest": "^3.2.3",
    "vue-tsc": "^3.0.6",
    "workbox-window": "^7.3.0"
  },
  "volta": {
    "node": "22.13.1",
    "npm": "8.19.4"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
