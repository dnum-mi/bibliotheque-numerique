{
  "name": "@biblio-num/client",
  "version": "0.0.0",
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "dev": "vite --open",
    "docker:dev": "docker compose -f docker-compose.dev.yml up",
    "docker:dev:build": "docker compose -f docker-compose.dev.yml build",
    "docker:dev:stop": "docker compose -f docker-compose.dev.yml down",
    "docker:prod": "docker compose -f docker-compose.prod.yml up -d",
    "docker:prod:build": "docker compose -f docker-compose.prod.yml build",
    "docker:prod:stop": "docker compose -f docker-compose.prod.yml down",
    "format": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "pretest:e2e": "npm run build",
    "pretest:e2e:ci": "npm run build",
    "preview": "vite preview --port 5050",
    "test": "run-s test:unit test:ct:ci",
    "test:ct": "cypress open --component",
    "test:ct:ci": "cypress run --component",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "test:unit": "vitest --environment jsdom",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "dependencies": {
    "@gouvfr/dsfr": "^1.9.2",
    "@gouvminint/vue-dsfr": "^3.6.0",
    "@vee-validate/zod": "^4.8.6",
    "ag-grid-community": "^28.2.1",
    "ag-grid-enterprise": "^28.2.1",
    "ag-grid-vue3": "^28.2.1",
    "axios": "^1.4.0",
    "oh-vue-icons": "1.0.0-rc3",
    "pinia": "^2.0.35",
    "pretty-bytes": "^6.1.0",
    "vee-validate": "^4.8.6",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@cypress/vue": "^5.0.5",
    "@faker-js/faker": "^7.6.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "@testing-library/vue": "^7.0.0",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.16.3",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.3.2",
    "cypress": "^12.11.0",
    "eslint": "^8.39.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.11.0",
    "jsdom": "^22.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "start-server-and-test": "^2.0.0",
    "typescript": "~4.9.5",
    "vite": "^4.3.4",
    "vite-plugin-pwa": "^0.14.7",
    "vitest": "^0.30.1",
    "vue-tsc": "^1.6.3",
    "workbox-window": "6.5.4"
  },
  "volta": {
    "node": "18.15.0",
    "npm": "8.19.4"
  }
}