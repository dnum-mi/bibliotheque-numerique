{
  "name": "@biblio-num/client",
  "version": "0.0.0",
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "dev": "vite",
    "dev:e2e": "start-server-and-test 'vite dev --port 5050' http://127.0.0.1:5050/ 'cypress open --e2e'",
    "docker:dev": "docker compose -f docker-compose.dev.yml up",
    "docker:dev:build": "docker compose -f docker-compose.dev.yml build",
    "docker:dev:stop": "docker compose -f docker-compose.dev.yml down",
    "docker:prod": "docker compose -f docker-compose.prod.yml up -d",
    "docker:prod:build": "docker compose -f docker-compose.prod.yml build",
    "docker:prod:stop": "docker compose -f docker-compose.prod.yml down",
    "format": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:staged": "eslint --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "pretest:e2e": "npm run build",
    "pretest:e2e:ci": "npm run build",
    "preview": "vite preview --port 5050",
    "test": "run-s test:unit test:ct:ci",
    "test:ct": "cypress open --component",
    "test:ct:ci": "cypress run --component",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run --e2e'",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "dependencies": {
    "@biblio-num/shared": "workspace:^",
    "@gouvfr/dsfr": "^1.9.3",
    "@gouvminint/vue-dsfr": "^4.3.0",
    "@vee-validate/zod": "^4.11.1",
    "@vueuse/core": "10.3.0",
    "ag-grid-community": "^29.3.5",
    "ag-grid-enterprise": "^29.3.5",
    "ag-grid-vue3": "^29.3.5",
    "axios": "^1.4.0",
    "oh-vue-icons": "1.0.0-rc3",
    "pinia": "^2.1.6",
    "pretty-bytes": "^6.1.1",
    "vee-validate": "^4.11.1",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "zod": "^3.22.1"
  },
  "devDependencies": {
    "@cypress/vue": "^5.0.5",
    "@dnum-mi/ds-api-client": "^1.5.1",
    "@faker-js/faker": "^7.6.0",
    "@rushstack/eslint-patch": "^1.3.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "@testing-library/vue": "^7.0.0",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.16.18",
    "@vitejs/plugin-vue": "^4.3.1",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "cypress": "^12.17.4",
    "eslint": "^8.47.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-cypress": "^2.14.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.2",
    "sass": "^1.65.1",
    "start-server-and-test": "^2.0.0",
    "typescript": "~4.9.5",
    "unocss": "0.55.1",
    "vite": "^4.4.9",
    "vite-plugin-pwa": "^0.16.4",
    "vite-svg-loader": "4.0.0",
    "vitest": "^0.34.2",
    "vue-tsc": "^1.8.8",
    "workbox-window": "7.0.0"
  },
  "volta": {
    "node": "18.16.0",
    "npm": "8.19.4"
  }
}